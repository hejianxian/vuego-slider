{
  "name": "vuego-slider",
  "description": "A Vue.js Slider Component",
  "version": "0.1.1",
  "author": "Hejx <531601727@qq.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build:demo": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "rollup -c --environment BUILD:production",
    "test": "jest"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.19",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jest": "^23.1.0",
    "jest-serializer-vue": "^2.0.2",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "rollup": "^0.61.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-vue": "^4.2.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
